   "source": [
    "n = 5  # 總共的行數\n",
    "space = '_'\n",
    "for i in range(n):  # 對於每一行\n",
    "    # 印出空格\n",
    "    print(space * (n - i - 1), end='')  # 在每行前面印出空格\n",
    "    # 印出星號\n",
    "    print('*' * (2 * i + 1))  # 每行印出 2*i +\n"
   ]
